{
  "name": "trackqueen",
  "version": "1.0.0",
  "description": "Learn more about your music.",
  "scripts": {
    "build": "npx eas-cli build --platform ios",
    "start": "npx expo start --ios -c",
    "submit": "npx eas-cli submit -p ios",
    "test": "jest",
    "eject": "expo eject",
    "format": "prettier --write *.{js,json}",
    "lint": "eslint .",
    "ios": "expo run:ios",
    "android": "expo run:android"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^1.1.0",
    "@expo/ui": "~0.2.0-beta.9",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-segmented-control/segmented-control": "^2.5.7",
    "@react-navigation/bottom-tabs": "^7.8.11",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/native-stack": "^7.8.5",
    "@react-navigation/stack": "^7.6.11",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^4.35.3",
    "crypto-js": "^4.2.0",
    "expo": "~54.0.33",
    "expo-asset": "~12.0.12",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-crypto": "~15.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "~19.0.21",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-bottom-tabs": "^1.1.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^4.2.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.17",
    "prettier": "^3.8.1",
    "react-test-renderer": "^19.2.4"
  },
  "author": "John Canelis",
