{
  "name": "trackqueen",
  "version": "1.0.0",
  "description": "Learn more about your music.",
  "scripts": {
    "build": "npx eas-cli build --platform ios",
    "start": "npx expo start --ios -c",
    "submit": "npx eas-cli submit -p ios",
    "test": "jest",
    "eject": "expo eject",
    "format": "prettier --write *.{js,json}",
    "lint": "eslint .",
    "ios": "expo run:ios",
    "android": "expo run:android"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^1.1.0",
    "@expo/ui": "~55.0.0-preview.6",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native-segmented-control/segmented-control": "^2.5.7",
    "@react-navigation/bottom-tabs": "^7.8.11",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/native-stack": "^7.8.5",
    "@react-navigation/stack": "^7.6.11",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^4.35.3",
    "crypto-js": "^4.2.0",
    "expo": "^55.0.0-preview.11",
    "expo-asset": "~55.0.5",
    "expo-audio": "~55.0.6",
    "expo-auth-session": "~55.0.4",
    "expo-blur": "~55.0.6",
    "expo-build-properties": "~55.0.7",
    "expo-crypto": "~55.0.6",
    "expo-dev-client": "~55.0.6",
    "expo-file-system": "~55.0.6",
    "expo-font": "~55.0.4",
    "expo-image": "~55.0.3",
    "expo-linking": "~55.0.4",
    "expo-secure-store": "~55.0.6",
    "expo-splash-screen": "~55.0.6",
    "expo-status-bar": "~55.0.3",
    "expo-symbols": "~55.0.4",
    "expo-system-ui": "~55.0.6",
    "expo-updates": "~55.0.8",
    "expo-web-browser": "~55.0.6",
    "prop-types": "^15.8.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-bottom-tabs": "^1.1.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-pager-view": "8.0.0",
    "react-native-reanimated": "~4.2.1",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-tab-view": "^4.2.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@eslint/compat": "^2.0.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "jest": "~29.7.0",
    "jest-expo": "~55.0.6",
