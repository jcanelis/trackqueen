{
  "name": "trackqueen",
  "version": "1.0.0",
  "description": "Learn more about your music.",
  "scripts": {
    "build": "npx eas-cli build --platform ios",
    "start": "npx expo start --ios -c",
    "submit": "npx eas-cli submit -p ios",
    "test": "jest",
    "eject": "expo eject",
    "format": "prettier --write *.{js,json}",
    "lint": "eslint .",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-segmented-control/segmented-control": "2.5.4",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@shopify/flash-list": "1.7.3",
    "@tanstack/react-query": "^4.35.3",
    "crypto-js": "^4.2.0",
    "expo": "~52.0.46",
    "expo-asset": "~11.0.5",
    "expo-auth-session": "~6.0.3",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.2",
    "expo-crypto": "~14.0.2",
    "expo-dev-client": "~5.0.20",
    "expo-file-system": "~18.0.12",
    "expo-image": "~2.0.7",
    "expo-linking": "~7.0.5",
    "expo-secure-store": "~14.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "^2.20.2",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-tab-view": "^4.0.1",
    "react-native-web": "^0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1"
  },
  "author": "John Canelis",
  "license": "MIT",
  "private": true
}
