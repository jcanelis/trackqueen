{
  "name": "trackqueen",
  "version": "1.0.0",
  "description": "Learn more about your music.",
  "scripts": {
    "build": "npx eas-cli build --platform ios",
    "start": "npx expo start --ios -c",
    "submit": "npx eas-cli submit -p ios",
    "test": "jest",
    "eject": "expo eject",
    "format": "prettier --write *.{js,json}",
    "lint": "eslint .",
    "ios": "expo run:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^0.11.2",
    "@react-native-masked-view/masked-view": "0.3.2",
    "@react-native-segmented-control/segmented-control": "2.5.7",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "@react-navigation/native-stack": "^7.0.0",
    "@react-navigation/stack": "^7.0.0",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^4.35.3",
    "crypto-js": "^4.2.0",
    "expo": "^54.0.0",
    "expo-asset": "~12.0.9",
    "expo-auth-session": "~7.0.8",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-build-properties": "~1.0.9",
    "expo-crypto": "~15.0.7",
    "expo-dev-client": "~6.0.12",
    "expo-file-system": "~19.0.15",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-updates": "~29.0.11",
    "expo-web-browser": "~15.0.7",
    "prop-types": "^15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-bottom-tabs": "^0.11.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-tab-view": "^4.0.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.12",
    "prettier": "^3.3.3",
    "react-test-renderer": "^18.3.1"
  },
  "author": "John Canelis",
  "license": "MIT",
  "private": true
}
