{
  "expo": {
    "name": "TrackQueen",
    "slug": "tq22",
    "version": "1.0.8",
    "orientation": "portrait",
    "description": "Learn more about your music.",
    "scheme": "tq22",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "githubUrl": "https://github.com/jcanelis/trackqueen",
    "owner": "jcanelis",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "icon": "./assets/app.icon",
      "bundleIdentifier": "com.jcanelis.tq22",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "audio",
          "fetch"
        ],
        "NSMicrophoneUsageDescription": "Enable the microphone to find songs using nearby audio.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "usesIcloudStorage": false,
      "usesAppleSignIn": false,
      "accessesContactNotes": false
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/4e40a877-6f31-41c4-a1e8-446cb9ce2fee"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "eas": {
        "projectId": "4e40a877-6f31-41c4-a1e8-446cb9ce2fee"
      }
    },
    "plugins": [
      "react-native-bottom-tabs",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-secure-store",
      "expo-asset",
      "expo-web-browser"
    ],
    "runtimeVersion": "1.0.0",
    "android": {
      "package": "com.jcanelis.tq22"
    }
  }
}
